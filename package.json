{
  "name": "@companion/pack-protocol",
  "version": "0.1.0",
  "description": "Types and utilities for building Clip Companion game packs",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./vite": {
      "import": "./dist/vite-plugin.js",
      "types": "./dist/vite-plugin.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/vite-plugin.ts --format esm --dts",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "vite": "^5.0.0 || ^6.0.0"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vite": "^6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lucidiac/companion-pack-protocol"
  },
  "license": "MIT",
  "keywords": [
    "gamepack",
    "clip-companion",
    "vite-plugin",
    "sandbox"
  ]
}
